{
    "apiKeyHelper": "~/.claude/api-key-helper.sh",
    "permissions": {
        "allow": [
            "Bash(true)",
            "Bash(ls:*)",
            "Bash(find:*)",
            "Bash(grep:*)",
            "Bash(cat:*)",
            "Bash(head:*)",
            "Bash(tail:*)",
            "Bash(cut:*)",
            "Bash(rg:*)",
            "Bash(git status:*)",
            "Bash(git diff:*)",
            "Bash(gh issue view:*)",
            "Bash(gh pr view:*)",
            "Bash(gh pr diff:*)",
            "Bash(whois:*)",
            "Bash(python -m py_compile:*)",
            "Bash(python3 -m py_compile:*)",
            "Bash(cargo check:*)",
            "Bash(cargo clippy:*)",
            "WebFetch(domain:docs.rs)",
            "Bash(bash ~/.claude/commands/review-prompt.sh:*)",
            "Bash(bash jpenv-bin/lint.bash:*)",
            "Bash(bash jpenv-bin/tests.bash:*)"
        ]
    },
    "model": "sonnet",
    "autoUpdates": false
}

[core]
	editor = vim

[color]
	ui = always
	interactive = false

[alias]
	bl = blame --show-stats -s
	br = branch
	ci = commit -v
	co = checkout
	del = branch -d
	di = diff
	dst = diff --stat
	pa = add --patch
	ls = ls-tree -rt --name-only HEAD
	st = status
	sh = show
	shf = diff-tree --no-commit-id --name-only -r HEAD

[stoq]
        checkout = /home/jpmelos/devel
